<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Model\Story\Story" table="app_stories" repository-class="App\Repository\StoryRepository">
        <id name="no" type="integer">
            <generator strategy="AUTO"/>
        </id>

        <field name="id" type="guid" nullable="false" unique="true"/>
        <field name="title" type="string" nullable="false"/>
    </entity>
</doctrine-mapping>
